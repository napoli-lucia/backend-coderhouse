<h1>Productos</h1>

{{#each products}}
<div class="container">
  <div class="producto">
    <!-- <img src="../public/imgs/square.png" alt="Producto x"> -->
    <div class="info">
      <ul>
        <li>ID: {{this.id}}</li>
        <li>Titulo: {{this.title}}</li>
        <li>Descripcion: {{this.description}}</li>
        <li>Codigo: {{this.code}}</li>
        <li>Stock: {{this.stock}}</li>
        <li>Categoria: {{this.category}}</li>
        <li>Precio: ${{this.price}}</li>
      </ul>
    </div>
  </div>
</div>
{{/each}}